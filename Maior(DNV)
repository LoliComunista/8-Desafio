package pogs;

import java.util.Scanner;

public class Pogs {

    public static void main(String[] args) {
        //variaveis
        Scanner ler = new Scanner(System.in);
        numMaior();
    }

    public static void numMaior() {
        //variaveis
        Scanner ler = new Scanner(System.in);
        int num1, num2, num3;

        //dados
        System.out.println("Insira três números");
        num1 = ler.nextInt();
        num2 = ler.nextInt();
        num3 = ler.nextInt();
        //processo
        if (num1 > num2 && num1>num3) {
            System.out.println(num1 + " Maior");
        } else if (num2 > num3) {
            System.out.println(num2 + " Maior");
        } else {
            System.out.println(num3 + " Maior");

        }
    }

}
